
type link = (i32, i32)

let main(from:[]i32, to:[]i32, degrees:[]i32): i32 = length from

-- Add contributions from pages without any outbound edges
-- This defaults to the page contribution / number of pages
let contribution_dangle [n] (ranks:[]f32, sizes:[n]i32) : []f32 = ranks

-- Add contributions from pages
let contribution [n] (links:[]link, ranks: []f32, sizes:[n]i32) : []f32 = ranks
